{
   "type": "sqlite",
   "database": "./database.sqlite",
   "synchronize": true,
   "logging": true,
   "entities": [
      "typescript-source/server/db/entity/**/*.ts"
   ],
   "migrations": [
      "typescript-source/server/db/migration/**/*.ts"
   ],
   "subscribers": [
      "typescript-source/server/db/subscriber/**/*.ts"
   ],
   "cli": {
      "entitiesDir": "typescript-source/server/db/entity",
      "migrationsDir": "typescript-source/server/db/migration",
      "subscribersDir": "typescript-source/server/db/subscriber"
   }
}